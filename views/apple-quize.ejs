<%- include("partials/header") -%>

<h3>Read instructions below carefully</h3>
<br>
<h5>1. If id is locked or not working then report on discord server or retry after 8 hours. ID refresh every 8 hours.</h5>
<h5>2. Do not login to icloud.</h5>
<h5>3. Apple Id password or email changes so get new one everytime and use apple id fast.</h5>
<h5>4. Do not share apple id with anyone.</h5>


<div class="eap">
  <label for="">Email</label>
  <input type="text"  value="" id="email" readonly="readonly">
  <button onclick="copy1()">Click To Copy</button>
  <label for="">Password</label>
  <input type="text"  value="" id="pass" readonly="readonly">
  <button onclick="copy2()">Click To Copy</button>
</div>

<script type="text/javascript">
console.info(performance.navigation.type);
if (performance.navigation.type == performance.navigation.TYPE_RELOAD) {
  console.info( "This page is reloaded" );
} else {
  console.info( "This page is not reloaded");
  if (document.referrer === "https://www.akwebguide.com/") {
var a = <%- JSON.stringify(email) %>
var b = <%- JSON.stringify(pass) %>
   document.getElementById("email").value = a
   document.getElementById("pass").value = b
}
}

function copy1() {
  var copyText = document.getElementById("email");
  copyText.select();
  copyText.setSelectionRange(0, 99999);
  navigator.clipboard.writeText(copyText.value);
  alert("Copied the text: " + copyText.value);
}

function copy2() {
  var copyText = document.getElementById("pass");
  copyText.select();
  copyText.setSelectionRange(0, 99999);
  navigator.clipboard.writeText(copyText.value);
  alert("Copied the text: " + copyText.value);
}
</script>


<%- include("partials/footer") -%>
